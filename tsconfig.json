{
    "compilerOptions": {
      "types": [
        "vite/client",
        "jest"
      ],
      "rootDir": ".",
      "baseUrl": ".",
      "target": "ES2020",
      "useDefineForClassFields": true,
      "module": "ESNext",
      "lib": [
        "ES2020",
        "DOM",
        "DOM.Iterable"
      ],
      
      "skipLibCheck": true,
      /* Bundler mode */
      "moduleResolution": "bundler",
      "allowImportingTsExtensions": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      
      "noEmit": true,
      "paths": {
        // "@/*": [
        //   "./src/*"
        // ],
        "@f/*":[
          "./src/front/*"
        ],
        "@b/*":[
          "./src/back/*"
        ],
        "@t/*":[
          "./src/types/*"
        ],
        "@u/*":[
          "./src/utils/*"
        ],
      },
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.d.ts",
      "src/**/*.tsx",
      "src/**/*.vue",
      "vite.config.ts",
      "src/main.ts",
    ],  
  }